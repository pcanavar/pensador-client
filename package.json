{
  "name": "pensador-client",
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pcanavar/ts-pensador"
  },
  "description": "A simple package to get quotes from pensador.com.br",
  "keywords": ["pensador", "scraper"],
  "author": "Philippe Canavarro",
  "scripts": {
    "dev": "ts-node-dev --clear --rs --respawn src/ts/index.ts",
    "prebuild": "rimraf dist",
    "build": "tsc --build --force",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "test": "jest",
    "devtest": "jest --watchAll --verbose"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.31",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "jest": "^29.6.2",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "zod": "^3.21.4"
  }
}
